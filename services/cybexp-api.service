[Unit]
Description=uWSGI Cybex-P API
After=network.target

[Service]
#WorkingDirectory=/opt/cybexp/api
ExecStart=/opt/cybexp/env/bin/uwsgi --ini /etc/cybexp/api/api-uwsgi.ini 
#User=cybexp-api
#Group=cybexp-api
Restart=on-failure
RestartSec=1
KillSignal=SIGQUIT
Type=notify
ExecReload=/bin/kill -SIGHUP $MAINPID

[Install]
WantedBy=multi-user.target
